class Solution {
public:
    int search(vector<int>& nums, int target) {
        int st=0,ed=nums.size()-1;
        while(st<=ed)
        {
            int m=st+(ed-st)/2;
            if(nums.at(m)==target)
            return m;
            else if(nums.at(m)>target)
            ed=m-1;
            else
            st=m+1;
        }
        return -1;
    }
};
