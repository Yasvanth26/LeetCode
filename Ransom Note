class Solution {
public:
    bool canConstruct(string ransomNote, string magazine) {
        vector<int> Ran(26, 0);
        vector<int> Mag(26, 0);

        for (char ch : ransomNote)
            Ran[ch - 'a']++;

        for (char ch : magazine)
            Mag[ch - 'a']++;

        for (int i = 0; i < 26; i++) {
            if (Ran[i] > Mag[i])
                return false;
        }

        return true;
    }
};
